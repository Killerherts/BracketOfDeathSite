{
  "name": "bracketofdeathsite",
  "version": "1.0.0",
  "description": "Tennis tournament score tracking web application for the Bracket of Death tournament",
  "main": "dist/backend/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/backend/server.js",
    "dev": "nodemon src/backend/server.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "docker:build": "docker build -t bracketofdeathsite .",
    "docker:run": "docker run -p 3000:3000 bracketofdeathsite"
  },
  "repository": {
    "type": "git",
    "url": "git@github-Killerherts:Killerherts/BracketOfDeathSite.git"
  },
  "keywords": [
    "tennis",
    "tournament",
    "bracket",
    "scoring",
    "web-app"
  ],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "google-auth-library": "^9.4.0",
    "helmet": "^7.1.0",
    "mongodb": "^6.17.0",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.1"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/morgan": "^1.9.9",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "postcss": "^8.5.6",
    "supertest": "^6.3.3",
    "tailwindcss": "^4.1.11",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
